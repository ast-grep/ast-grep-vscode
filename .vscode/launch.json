{
  "version": "0.0.1",
  "configurations": [
    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Launch Client",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceRoot}"
        // "--disable-extensions"
      ],
      "preLaunchTask": "tasks: dev"
    },
    {
      "name": "Language Client E2E Test",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceRoot}",
        "--extensionTestsPath=${workspaceRoot}/out/test/index",
        "${workspaceRoot}/client/testFixture"
      ]
    }
  ]
}
